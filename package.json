{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:test": "vite --mode development --config vite.config.ts",
    "dev:prod": "vite --mode production --config vite.config.ts",
    "build": "tsc && vite build",
    "build:staging": "NODE_ENV=staging tsc && vite build --mode staging",
    "build:test": "NODE_ENV=development tsc && vite build --mode development",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:db": "echo 'Base de données de test: SecondDetailDeck (shxnokjzkfnreolujhew)'",
    "prod:db": "echo 'Base de données de production: DetailDeck (jtusofarsnwcfxnrvgus)'"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "@supabase/supabase-js": "^2.43.4",
    "globals": "^16.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
